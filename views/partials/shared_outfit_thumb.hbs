
{{!-- used in /outfit/fitting-room --}}
<div id="shared_outfit_thumb_framework">
<div class="framework_inside">
<div class="desc">
    <a href='/outfit/detail/{{this._id}}'><h4>{{this.title}}</h4></a>
    <div class="like_items_div">
        <p class="outfit_thumb_nr"><img src="../images/icon_heart.png" alt="x" class="heart_icon">{{outfit.liked_by.length}}</p>
