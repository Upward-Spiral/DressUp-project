<div id="home-frame">

    <div id="fitting-room-frame">
        <h2 class="mainH2">Your Fitting Room</h2>
        <div id="fitting-room-frame-inside">
            {{#each outfit}}
            <a href="/outfit/feed/{{this._id}}">
                <div class="shared-outfit-home">
                    <img src="/{{this.image}}" alt="">
                    <p>{{this.title}}</p>
                </div>
            </a>     
            {{/each}} 
        </div>
    </div>

    <div id="fitting-room-frame">
        <h2 class="mainH2">Your Genies</h2>
        <div id="fitting-room-frame-inside">
            {{#each friendsList}}
            <a href="/genie/fitting-room/{{this._id}}">
                <div class="shared-outfit-home">
                    <img src="/{{this.image}}" alt="">
                    <p>{{this.firstname}}</p>
                </div>
            </a>
            {{/each}}
        </div>
    </div>
